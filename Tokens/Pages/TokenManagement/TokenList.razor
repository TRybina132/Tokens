@page "/TokenList"

<MudTable Items="@_tokens" Hover="true" Striped="true">
    <ToolBarContent>
        <MudText Typo="Typo.h6">Saved tokens</MudText>
    </ToolBarContent>
    <HeaderContent>
        <MudTh>Name</MudTh>
        <MudTh>Date</MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd DataLabel="Name">@context.Name</MudTd>
        <MudTd DataLabel="Creation date">@context.CreationDate.ToShortDateString()</MudTd>
    </RowTemplate>
</MudTable>